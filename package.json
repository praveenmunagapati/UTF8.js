{
  "name":           "uupaa.utf8.js",
  "version":        "0.8.11",
  "description":    "Convert UTF16(JavaScript String) to UTF8.",
  "url":            "https://github.com/uupaa/UTF8.js",
  "keywords":       ["utf16", "utf8", "util", "uupaa"],
  "repository": {
    "type":         "git",
    "url":          "https://github.com/uupaa/UTF8.js.git"
  },
  "scripts": {
    "watch":        "node node_modules/uupaa.watch.js --verbose --action build ./lib/UTF8.js",
    "build":        "node node_modules/uupaa.minify.js --keep --option 'formatting pretty_print' --output ./lib/UTF8.min.js ./lib/UTF8.js",
    "build-release":"node node_modules/uupaa.minify.js --module all --output ./lib/UTF8.min.js",
    "test":         "NODE_ENV=production NODE_PATH=lib node --harmony test/index.node.js; open test/index.html",
    "lint":         "plato --title UTF8.js --jshint .jshintrc -d lint/plato lib/UTF8.js; open lint/plato/index.html"
  },
  "build": {
    "all": { "lib/UTF8.js": "" },
    "node": { },
    "worker": { },
    "browser": { }
  },
  "devDependencies": {
    "uupaa.valid.js":   "",
    "uupaa.help.js":    "",
    "uupaa.task.js":    "",
    "uupaa.test.js":    "",
    "uupaa.minify.js":  "",
    "uupaa.bytearray.js": "",
    "uupaa.wordarray.js": ""
  },
  "lib":            "./lib/",
  "main":           "./index.js",
  "author":         "uupaa <uupaa.js@gmail.com>",
  "license":        "MIT",
  "contributors":   []
}
